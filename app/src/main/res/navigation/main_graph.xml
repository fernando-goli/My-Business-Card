    </fragment>
    <fragment
        android:id="@+id/addBusinessCardFragment"
        android:name="com.fgomes.mybusinesscard.ui.addCard.AddBusinessCardFragment"
        android:label="add_business_card_fragment"
        tools:layout="@layout/add_business_card_fragment" />
</navigation>