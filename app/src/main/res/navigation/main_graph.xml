    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_graph"
    app:startDestination="@id/businessCardListFragment">
    <fragment
        android:id="@+id/businessCardListFragment"
        android:name="com.fgomes.mybusinesscard.ui.home.BusinessCardListFragment"
        android:label="BusinessCardListFragment"
        tools:layout="@layout/business_card_list_fragment">
        <action
            android:id="@+id/action_businessCardListFragment_to_addBusinessCardFragment"
